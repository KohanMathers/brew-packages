import ./unix;

function time() {
  return timestamp();
}

function fromUnix(time) {
  unix.fromUnix(time);
}

function daysInYear(y) {
  if (isLeapYear(y)) {
    return 366;
  } else {
    return 365;
  }
}

function isLeapYear(y) {
    if (y  % 400 == 0) {
        return true;
    }
    if (y % 100 == 0) {
        return false;
    }
    if (y % 4 == 0) {
        return true;
    }
    else {
        return false;
    }
}
